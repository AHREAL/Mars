{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "jsx": "react",
    "noEmit": true,
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@cpn": [
        "./src/components/index"
      ],
      "@page": [
        "./src/pages/index"
      ],
      "@page/*": [
        "./src/pages/*"
      ],
      "@hook": [
        "./src/hooks/index"
      ],
      "@router": [
        "./src/router/index"
      ],
      "@router/*": [
        "./src/router/*"
      ]
    },
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}